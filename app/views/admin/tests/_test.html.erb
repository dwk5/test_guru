<tr>
  <td><%= test.title %></td>
  <td align="center"><%= test.level %></td>
  <td align="center"><%= test.questions.size%></td>
  <td><%= link_to t('.show_test'), admin_test_path(test), class: 'btn btn-primary' %></td>
  <td><%= link_to t('.edit_test'), edit_admin_test_path(test), class: 'btn btn-warning' %></td>
  <td>
    <%= link_to t('.delete_test'),
                  admin_test_path(test),
                  class: 'btn btn-danger',
                  method: :delete,
                  data: {confirm: 'Are you sure?' } %>
  </td>
</tr>
